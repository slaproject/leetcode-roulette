{
    "version": 2,
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "https://leetcode-roulette.onrender.com/api/$1"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "X-Frame-Options", "value": "SAMEORIGIN" },
          { "key": "X-Content-Type-Options", "value": "nosniff" }
        ]
      }
    ]
  }